// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")

    relationMode = "prisma"
}

model User {
    id        String @id @unique
    email     String @unique
    name      String 
    problems  String
    items     String
    gold      Int
    armor     Int
    accessory Int
    weapon    Int
    focus     Int
    skill1    Int
    skill2    Int
    skill3    Int
    score     Int
    time      DateTime
}
